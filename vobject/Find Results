  584      getCenter: function getCenter(touches) {
  585:         var valuesX = [], valuesY = [];
  586
  587          for(var t= 0,len=touches.length; t<len; t++) {
  588:             valuesX.push(touches[t].pageX);
  589:             valuesY.push(touches[t].pageY);
  590          }
