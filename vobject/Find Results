   34  @register.filter
   35: def humanized_simple_join(value):
   36      """
   37      Given a list of strings, format them with commas and spaces,
   ..
   49      """
   50      # make everything a string to avoid errors
   51:     value = [six.u(item) for item in value]
   52
   53:     if len(value) == 1: # one item in list
   54:         return value[0]
   55:     if len(value) == 2: # two items in list
   56:         return "%s and %s" % (value[0], value[1])
   57
   58      # join all but the last element
   59:     all_but_last = ", ".join(value[:-1])
   60:     return "%s, and %s" % (all_but_last, value[-1])
   61
