  686  # the combined Python string replacement and regex syntax is a little confusing;
  687  # remember that %(foobar)s is replaced with patterns['foobar'], so for instance
  688: # param_value is any number of safe_chars or any number of qsaf_chars surrounded
  689  # by double quotes.
  690
  691: patterns['param_value'] = ' "%(qsafe_char)s * " | %(safe_char)s * ' % patterns
  692
  693
  694: # get a tuple of two elements, one will be empty, the other will have the value
  695: patterns['param_value_grouped'] = """
  696  " ( %(qsafe_char)s * )" | ( %(safe_char)s + )
  697  """ % patterns
  698
  699: # get a parameter and its values, without any saved groups
  700  patterns['param'] = r"""
  701  ; (?: %(name)s )                     # parameter name
  702  (?:
  703:     (?: = (?: %(param_value)s ) )?   # 0 or more parameter values, multiple
  704:     (?: , (?: %(param_value)s ) )*   # parameters are comma separated
  705  )*
  706  """ % patterns
  707
  708: # get a parameter, saving groups for name and value (value still needs parsing)
  709  patterns['params_grouped'] = r"""
  710  ; ( %(name)s )
  ...
  712  (?: =
  713      (
  714:         (?:   (?: %(param_value)s ) )?   # 0 or more parameter values, multiple
  715:         (?: , (?: %(param_value)s ) )*   # parameters are comma separated
  716      )
  717  )?
  718  """ % patterns
  719
  720: # get a full content line, break it up into group, name, parameters, and value
  721  patterns['line'] = r"""
  722  ^ ((?P<group> %(name)s)\.)?(?P<name> %(name)s) # name group
  723    (?P<params> (?: %(param)s )* )               # params group (may be empty)
  724: : (?P<value> .* )$                             # value group
  725  """ % patterns
  726
  727  ' "%(qsafe_char)s*" | %(safe_char)s* '
  728
  729: param_values_re = re.compile(patterns['param_value_grouped'], re.VERBOSE)
  730  params_re       = re.compile(patterns['params_grouped'],      re.VERBOSE)
  731  line_re         = re.compile(patterns['line'],    re.DOTALL | re.VERBOSE)
