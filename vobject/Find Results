  143: def valuesToDict(key):
  144:     """Convert a registry key's values to a dictionary."""
  145      dict={}
  146      size=_winreg.QueryInfoKey(key)[1]
  147      for i in xrange(size):
  148:         dict[_winreg.EnumValue(key, i)[0]]=_winreg.EnumValue(key, i)[1]
  149      return dict
